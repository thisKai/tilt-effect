{
  "source": "./src",
  "destination": "./docs",
  "includes": [
    "effect.*\\.js$"
  ],
  "plugins": [
    {"name": "esdoc-standard-plugin"},
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "stripPackageName": true,
        "replaces": [
          {"from": "^src/effect.js", "to": "tilt-effect"}
        ]
      }
    }
  ]
}
